class Solution {
public:
    int arraySign(vector<int>& nums) {
        int n = nums.size();
        long long x = 1;;
        for(int i = 0;i<n;i++){
            if(nums[i]<0){
            x = -x;
        }
        else if (nums[i]==0){
            return 0;
        }
        }
        
        return x;
        //reutn
    }
};