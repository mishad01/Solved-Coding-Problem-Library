//Z. Hard Compare
//Given 4 numbers A,B,C
//        and D
//        . If AB
//        > CD
//        print "YES" otherwise, print "NO".
//
//        Input
//        Only one line containing 4 numbers A,B,C
//        and D
//        (1≤A,C≤107)
//        , (1≤B,D≤1012)
//        Output
//        Print "YES" or "NO" according to the problem above.
#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
	ll a,b,c,d ; 
	cin >> a >> b >> c >> d ; 
	if(b*log(a) > d * log(c)) printf("YES\n");
	else printf("NO\n");
	return 0 ; 
}
