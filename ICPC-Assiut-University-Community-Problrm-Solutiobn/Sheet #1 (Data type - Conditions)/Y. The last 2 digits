//Given 4 numbers A
//        , B
//        , C
//        and D
//        . Print the last 2 digits from their Multiplication.
//
//        Input
//        Only one line containing four numbers A
//        , B
//        , C
//        and D
//        (2≤A,B,C,D≤109)
//        .
//
//        Output
//        Print the last 2 digits from their Multiplication.
#include<iostream>
using namespace std;
int main ()
{
	int a,b,c,d;
	cin>>a>>b>>c>>d;
	a%=100;
	b%=100;
	c%=100;
	d%=100;
	int mult;
	mult= a*b*c*d;
	if(mult%100<=9)
	{
		cout<<0;
	}
	cout<<mult%100;
}
