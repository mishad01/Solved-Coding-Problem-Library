#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define nl '\n'
int main() {
	int q;
	cin >> q;
	while (q--) {
		int n;
		cin >> n;
		if (n == 0) {
			cout << 0 << '\n';
			continue;
		}
		while (n > 0) {
			int num = n % 10;
			cout << num << " ";
			n /= 10;
		}
		cout << nl;
	}
}