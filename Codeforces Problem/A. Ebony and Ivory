#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define nl '\n'
int main() {
    int a, b, c;
    cin >> a >> b >> c;
    for (int i = 0; i <= c; i += a) {
        for(int j = 0;j<=c;j+=b){
            if(i+j==c){
                cout<<"Yes"<<nl;
                return 0;
            }
        }
    }
    cout<<"No"<<nl;
    return 0;
}